package game;

import java.awt.*;
import javax.swing.*;

public class Game extends JPanel {
	public Game(){

	JPanel list = new JPanel();
	JPanel menus = new JPanel();
	JButton start = new JButton("start");
	JButton setting = new JButton("setting");
	JButton exit = new JButton("exit");

	
	
	this.add(list);
	this.add(menus);
	menus.add(start);
	menus.add(setting);
	menus.add(exit);

	}
}